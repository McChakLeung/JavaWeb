<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>com.dgpalife.controller.OneServlet</servlet-name>
    <servlet-class>com.dgpalife.servlet.com.dgpalife.controller.OneServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>com.dgpalife.controller.OneServlet</servlet-name>
    <url-pattern>/one.do</url-pattern>
  </servlet-mapping>
  
  <filter>
    <filter-name>OneFilter</filter-name>
    <filter-class>com.dgpalife.filter.OneFilter</filter-class>
  </filter>

  <filter-mapping>
    <filter-name>OneFilter</filter-name>
    <url-pattern>/one.do</url-pattern>
  </filter-mapping>
</web-app>
