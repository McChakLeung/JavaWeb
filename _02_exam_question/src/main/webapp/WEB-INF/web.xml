<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>QuestionAddServlet</servlet-name>
    <servlet-class>com.dgpalife.controller.question.QuestionAddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QuestionAddServlet</servlet-name>
    <url-pattern>/exam/question/add.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>QuestionFindServlet</servlet-name>
    <servlet-class>com.dgpalife.controller.question.QuestionFindServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QuestionFindServlet</servlet-name>
    <url-pattern>/exam/question/find.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>QuestionDeleteServlet</servlet-name>
    <servlet-class>com.dgpalife.controller.question.QuestionDeleteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QuestionDeleteServlet</servlet-name>
    <url-pattern>/exam/question/delete.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>QuestionFindByIdServlet</servlet-name>
    <servlet-class>com.dgpalife.controller.question.QuestionFindByIdServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QuestionFindByIdServlet</servlet-name>
    <url-pattern>/exam/question/findById.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>QuestionUpdateServlet</servlet-name>
    <servlet-class>com.dgpalife.controller.question.QuestionUpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QuestionUpdateServlet</servlet-name>
    <url-pattern>/exam/question/update.do</url-pattern>
  </servlet-mapping>
</web-app>
